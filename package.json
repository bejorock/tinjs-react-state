{
  "name": "@tinqjs/tinjs-react-state",
  "version": "1.0.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:typings",
    "build:cjs": "tsc --module commonjs --target esnext --outDir dist/cjs",
    "build:esm": "tsc --module es2015 --target esnext --outDir dist/esm",
    "build:typings": "tsc --declaration true --emitDeclarationOnly true --outDir dist/typings"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "react": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bejorock/tinjs-react-state.git"
  },
  "author": "bejorock"
}
